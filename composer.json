{
    "name": "zema888/zema-tree-bundle",
    "type": "symfony-bundle",
    "description": "This Symfony bundle integrates angular application and Gedmo Nested Set directly to Sonata Admin",
    "keywords": ["ajax", "asynchronous", "async", "tree", "nested", "set", "bundle", "symfony", "bundle", "doctrine", "sonataproject"],
    "homepage": "https://github.com/zema888/zema-tree-bundle.git",
    "license": "MIT",
    "authors": [
        {
            "name": "Vitaly Zemtsov",
            "email": "zevi888@gmail.com"
    	}
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.6.0",
        "stof/doctrine-extensions-bundle": "^1.2",
        "sonata-project/admin-bundle": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "fabpot/php-cs-fixer": "^1.10",
        "satooshi/php-coveralls": "^0.6.1"
    },
    "autoload": {
        "psr-4": { "Zema\\Bundle\\TreeBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
